{
  "name": "@abnerweb3/blockchain-sdk",
  "version": "0.0.1",
  "description": "Web3 multi chain SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abner-jie/abnerweb3-blockchain-sdk.git"
  },
  "license": "MIT",
  "keywords": [
    "web3",
    "blockchain",
    "sdk",
    "web3js"
  ],
  "engines": {
    "node": ">=19.0.0"
  },
  "main": "./dist/tsc/index.js",
  "types": "./dist/tsc/index.d.ts",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "test": "jest",
    "docs": "typedoc --entryPoints src/index.ts",
    "clean": "rm -rf dist build package",
    "build": "run-s lint clean test build:main",
    "build:main": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.10",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^5.45.0",
    "esbuild": "^0.15.18",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@mysten/bcs": "^1.1.0",
    "@mysten/sui": "^1.13.0",
    "@noble/curves": "^1.6.0",
    "@noble/hashes": "^1.5.0",
    "@scure/bip39": "^1.4.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.4",
    "tslib": "^2.8.0",
    "tweetnacl": "^1.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/abner-jie/abnerweb3-blockchain-sdk/issues"
  },
  "homepage": "https://github.com/abner-jie/abnerweb3-blockchain-sdk#readme",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "author": "@abner.sun"
}
